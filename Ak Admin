local function AC()
    while _G.AC == true do
    for i,v in pairs(workspace:GetChildren()) do
        if v:IsA("Model") then
            if game.Players:FindFirstChild(v.Name) then
                if v:FindFirstChildWhichIsA("Tool") then
                    local tool = v:FindFirstChildWhichIsA("Tool")
                    tool:Destroy()
                end
            end
        end
    end
    wait()
end
end


